<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE configuration>
<configuration>
    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%date{yyyy-MM-dd HH:mm:ss}] %-5level %logger.%method\(%file:%line\) - %message%n</pattern>
        </encoder>
    </appender>
    <root level="ERROR">
        <appender-ref ref="CONSOLE"/>
    </root>
</configuration>
